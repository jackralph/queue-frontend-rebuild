mixin header(options)

    header.page-header.page-header-dark.bg-gradient-primary-to-secondary(class=options.pageHeaderStyle + ' ' + options.pageHeaderBg, style=options.pageHeaderBgImg)

        if block
            block
        else
            p.text-center.py-5 No content provided

        //- SVG Border
        include includes/border-options.pug