mixin navbar(options)

    nav.navbar.navbar-marketing.navbar-expand-lg(class=options.navbarBg, class=options.navbarStyle, class=options.navbarPosition)
        div(class=options.navbarContainer)
            a.navbar-brand(href='landing-app-mobile.html', class=options.navbarBrandColor)
                img(src="../../../assets/img/queuepngwhite.png" alt="logo" style="height: 7em")
            button.navbar-toggler(type='button', data-toggle='collapse', data-target='#navbarSupportedContent', aria-controls='navbarSupportedContent', aria-expanded='false', aria-label='Toggle navigation')
                i(data-feather='menu')
            #navbarSupportedContent.collapse.navbar-collapse
                ul.navbar-nav.ml-auto.mr-lg-5
                    li.nav-item
                        a.nav-link(href='page-company-about.html')
                            | Try the Demo 
                    li.nav-item
                        a.nav-link(href='page-company-pricing.html')
                            | Pricing
                    li.nav-item
                        a.nav-link(href='')
                            | Contact Us
                    li.nav-item
                        a.nav-link(href='')
                            | Login            
                    
                a(href="https://shop.startbootstrap.com/sb-ui-kit-pro", class=options.navbarBtnColor).btn.rounded-pill.px-4.ml-lg-4
                    | Buy Now
                    i.fas.fa-arrow-right.ml-1